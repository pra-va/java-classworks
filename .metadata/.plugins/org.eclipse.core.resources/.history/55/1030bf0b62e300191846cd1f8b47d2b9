package prava;

public class Pratybos {

	public static void main(String[] args) {
//		nuliai();
//		istrizaine();
		istrizaines();

	}

	private static void nuliai() {
		for (int i=0; i<10; i++) {
			for (int j=0; j<10; j++) {
				System.out.print(0 + " ");
			}
			System.out.println();
		}
	}
	
	private static void istrizaine() {
		for (int i=0; i<10; i++) {
			for (int j=0; j<10; j++) {
				if(i == j) {
					System.out.print(1 + " ");
				} else {
					System.out.print(0 + " ");
				}
			}
			System.out.println();
		}
	}
	
	private static void istrizaines() {
		for (int i=0; i<10; i++) {
			for (int j=0; j<10; j++) {
				if(i == j || 9 - j == i) {
					System.out.print(1 + " ");
				} else {
					System.out.print(0 + " ");
				}
			}
			System.out.println();
		}
	}
	

}
