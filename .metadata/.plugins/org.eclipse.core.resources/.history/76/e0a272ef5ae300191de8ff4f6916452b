package prava;

public class ForStatement {

	public static void main(String[] args) {
		checkFor();
	}

	private static void checkFor() {
		for(int i=0; i<=10; i++) {
			System.out.print(i + " ");
		}
		System.out.println();
		for (int i=0; i<=10; ) {
			System.out.print(i + " ");
			i++;
		}
	}

}
