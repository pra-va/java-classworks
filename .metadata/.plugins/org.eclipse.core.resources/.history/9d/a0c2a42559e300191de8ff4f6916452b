package prava;

public class Switch {

	public static void main(String[] args) {
		checkSwitch();

	}

	private static void checkSwitch() {
		String color = "green";
		switch (color) {
		case "red":
			System.out.println("red");
			break;
		case "green":
			System.out.println("green");
			break;
		default:
			System.out.println("...");
		}

	}

}
